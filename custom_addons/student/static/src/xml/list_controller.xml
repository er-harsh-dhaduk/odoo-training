<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-inherit="web.ListView.Buttons" t-inherit-mode="extension">
        <xpath expr="//t[@t-if='nbSelected']" position="inside">
            <button type="button" t-on-click="wbButtonClickeEvent"
                    t-if="model.root.resModel == 'wb.student'" class="btn btn-primary" id="school_edit">
                <span>Edit School</span>
            </button>
        </xpath>
    </t>
</templates>