<?xml version="1.0" encoding="UTF-8" ?><odoo>    <record id="school_list_view" model="ir.ui.view">        <field name="name">school.list.view</field>        <field name="model">school.profile</field>        <field name="arch" type="xml">            <tree>                <field name="school_seq_name" />                <field name="name" string="From List Changed."/>                <field name="email" />                <field name="phone" />                <field name="currency_id" />            </tree>        </field>    </record>    <record id="school_form_view" model="ir.ui.view">        <field name="name">school.form.view</field>        <field name="model">school.profile</field>        <field name="arch" type="xml">            <form>                <sheet>                    <div class="oe_button_box" name="button_box">                        <button name="specialCommand" class="oe_stat_button" icon="fa-address-book-o" type="object" string="SC 0 0 vals" />                        <button name="specialCommand1" icon="fa-bandcamp" type="object" string="SC 1 id vals" />                        <button name="specialCommand2" class="oe_stat_button" icon="fa-quora" type="object" string="SC 2 id 0" />                        <button name="specialCommand3" icon="fa-snowflake-o" type="object" string="SC 3 id 0" />                        <button name="specialCommand4" class="oe_stat_button" icon="fa-thermometer-quarter" type="object" string="SC 4 id 0"/>                    </div>                    <h1><field name="school_seq_name" nolabel="1"/></h1>                    <group>                        <group>                            <field name="name" />                            <field name="currency_id" />                            <field name="email"                                   placeholder="abc@gmail.com"/>                            <field name="phone" />                            <field name="is_virtual_class"/>                            <field name="school_rank" />                            <field name="result"/>                            <field name="address"/>                            <field name="auto_rank" nolabel="1" />                        </group>                        <group>                            <field name="document" />                            <field name="estalish_date" />                            <field name="open_date"/>                            <field name="school_type" />                        </group>                    </group>                    <group>                        <field name="documents" filename="document_name"                               />                        <field name="document_name" invisible="1"/>                        <field name="school_image" widget="image" invisible="1"/>                        <field name="school_description"                               string="School Description"/>                    </group>                    <group>                    </group>                </sheet>                <div class="oe_chatter">                    <field name="message_follower_ids" />                    <field name="message_ids"/>                </div>            </form>        </field>    </record>    <record id="action_school_profile" model="ir.actions.act_window">        <field name="name">School Profile</field>        <field name="res_model">school.profile</field>        <field name="view_mode">tree,form</field>        <field name="context">{'active_test':True}</field>    </record>    <menuitem id="school_root_menu" name="School" web_icon="school,static/description/icon.png"/>    <menuitem id="school_sub_menu" parent="school_root_menu"              name="School Profile"/>    <menuitem id="school_profile_menu" name="School"              action="action_school_profile"              parent="school_sub_menu"/>     <record id="school_student_profile_security_list_view" model="ir.ui.view">        <field name="name">school.student.profile.security.list.view</field>        <field name="model">school.student.profile.security.example</field>        <field name="arch" type="xml">            <tree>                <field name="name" />                <field name="email" />                <field name="phone" />            </tree>        </field>    </record>    <record id="school_student_profile_security_form_view" model="ir.ui.view">        <field name="name">school.student.profile.security.form.view</field>        <field name="model">school.student.profile.security.example</field>        <field name="arch" type="xml">            <form>                <field name="name" />                <field name="email" />                <field name="phone" />            </form>        </field>    </record>    <record id="action_school_student_security_profile" model="ir.actions.act_window">        <field name="name">School Student Profile</field>        <field name="res_model">school.student.profile.security.example</field>        <field name="view_mode">tree,form</field>    </record>    <menuitem id="school_student_securityprofile_menu" name="School Student Profile"              action="action_school_student_security_profile"              parent="school_sub_menu"/></odoo>